{% schema %}
  {
    "name": "Slideshow",
    "class": "index-section",
	"disabled_on": {
        "groups": ["header", "footer"]
    },
    "settings": [
	 {
        "type": "paragraph",      
        "content": "Use this slideshow when you have image + text to display above image."
      },		  	   
	  {
            "type": "select",
            "id": "top-offeset",
            "label": "Top OffSet",
            "default": "none",
			"info":" This leave space above section",
            "options": [
              {
                "value": "default",
                "label": "Default"
              },
              {
                "value": "small",
                "label": "Small"
              },
			  {
                "value": "none",
                "label": "None"
              }
			]
        },
	  {
        "type": "checkbox",
        "id": "sld",
        "label": "Hide on Desktop?",
        "default": false
      },
	  {
        "type": "checkbox",
        "id": "slm",
        "label": "Hide on mobile?",
        "default": false
      },
     {
		"type":"checkbox",
		"id":"section_width",
		"label":"Box Width?",
		"default": false	
	  },
	 {
        "type": "range",
        "id": "box-width",
       	"label":"Set Box Width",
        "default": 1500,
        "min": 1200,
        "max": 2200,
        "step": 100,
        "unit": "px"
      },
     {
		"type":"paragraph",
		"content":"Section Height ==="
	  },       
       {
            "type": "select",
            "id": "ht",
            "label": "Section Height ====",
            "default": "2",
            "options": [
                {
                    "value": "1",
                    "label": "Adapt to first image"
                },
                {
                    "value": "2",
                    "label": "Custom as below"
                }
            ]
      },
      {
        "type": "range",
        "id": "ds_height",
        "label": "Desktop",
        "default": 700,
        "min": 200,
        "max": 1000,
        "step": 10
      },
      {
        "type": "range",
        "id": "tb_height",
        "label": "Tablet",
        "default": 450,
        "min": 200,
        "max": 1000,
        "step": 10
      },
      {
        "type": "range",
        "id": "mb_height",
        "label": "Mobile",
        "default": 350,
        "min": 100,
        "max": 800,
        "step": 10
      },       
	 {
		"type":"paragraph",
		"content":"Slider Settings ==="	
	  },
      {
        "type": "select",
        "id": "slideEf",
        "label": "Slide Effect",
        "default": "true",
        "options": [
          { "value": "true", "label": "Fade" },
          { "value": "false", "label": "Slide" }
        ]
      },
	  {
        "type": "checkbox",
        "id": "autoplay",
        "label": "Auto-rotate slides",
        "default": true
      },
	  {
          "type": "range",
          "id": "speed",
          "label": "Change Slides Every",
          "default": 5,
          "min": 1,
          "max": 60,
          "step": 1,
          "unit": "sec"
      },
	  {
        "type": "checkbox",
        "id": "arrow",
        "label": "Show Next/Previous Arrows?",
        "default": true
      },
		{
        "type": "checkbox",
        "id": "arrow_m",
        "label": "Hide on mobile?",
        "default": true
      },
      {
        "type": "color",
        "id": "ss_arrow",
        "label": "Arrows Color",
        "default": "#eee"
      },
	  {
        "type": "checkbox",
        "id": "dots",
        "label": "Show Dotted Navigation?",
        "default": false
      },
 	  {
        "type": "checkbox",
        "id": "dots_m",
        "label": "Hide on mobile?",
        "default": true
      },
        {
          "type": "color",
          "id": "ss_dot",
          "label": "Dots Color",
          "default": "#eee"
        },   
		{
          "type": "color",
          "id": "ss_dota",
          "label": "Active Dot Color",
          "default": "#eee"
        },   
	    {
        "type": "checkbox",
        "id": "txtanimation",
        "label": "Enable Text Animation",
        "default": true
      },
      {
        "type": "select",
        "id": "animation-style",
        "label": "Text Animation Style",
        "default": "st1",
        "options": [
          {
            "label": "Style1",
            "value": "st1"
          },
          {
            "label": "Style2",
            "value": "st2"
          },
		  {
            "label": "Style3",
            "value": "st3"
          },
 		  {
            "label": "Style4",
            "value": "st4"
          } 
        ]
      }	 
    ],
    "blocks": [
