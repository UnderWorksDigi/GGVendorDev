{% assign secSt = section.settings %}
{%- capture style -%}
--cl:{{ secSt.ss_arrow}};
--dotbg:{{ secSt.ss_dot}};
--dotbga:{{ secSt.ss_dota}};
    {% if section.settings.ht == '1' %}
        {% for block in section.blocks %}
            {% if block.settings.image != blank %}
                --dh:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;
                {% if block.settings.mb_image != blank %}
                    --mh:{{ 1 | divided_by: block.settings.mb_image.aspect_ratio | times: 100}}%;
                {% else %}
                    --mh:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;
                {% endif %}
                {% break %}
            {% else %}
                --dh:40%;--mh:40%;
            {% endif %}
        {% endfor %}
    {% else %}
        --dh:{{secSt.ds_height}}px;--th:{{secSt.tb_height}}px;--mh:{{secSt.mb_height}}px;
    {% endif %}
{%- endcapture -%}
{%- capture slideShowSec -%}
<div class="content-indent {{ secSt.top-offeset}}{% if secSt.sld %} medium-up--hide{% endif %}{% if secSt.slm %} small--hide{% endif %}" data-section-id="{{ section.id }}" data-section-type="slideshow">
  
{% if section.blocks.size > 0 %}
  <div class="oss_wrap{% if secSt.section_width %} page-width{% endif %}"{% if secSt.section_width %} style="max-width:{{ secSt.box-width}}px; margin:0 auto"{% endif %}>
 
    <div class="slideshow{% if section.settings.ht == '1' %} im{% else %} ct{% endif %}{% if secSt.arrow_m %} hide_ar{% endif %}{% if secSt.dots_m %} hide_dt{% endif %}" id="Slideshow-{{ section.id }}" data-slick='{"slidesToShow":1,"slidesToScroll":1,"arrows":{{ secSt.arrow }},"dots":{{ secSt.dots }},"fade":{{secSt.slideEf}},{%- if settings.rtl -%}"rtl": true,{%- endif -%} "infinite": true, "autoplay":{{secSt.autoplay}}, "autoplaySpeed":{{secSt.speed | times: 1000}} }' style="{{style}}">
           
      {% for block in section.blocks %}
      
      {%- liquid
            assign blkSt = block.settings
        	if blkSt.video_url.type == "youtube"
        		assign videotype = "youtube"
        	elsif blkSt.video_url.type == "vimeo"
        		assign videotype = "vimeo"
        	else
        		assign videotype = "video"
            endif
            
            if blkSt.mb_image != blank
            	assign dsizes = '1296,1512,2000,2450,3000,3600'
            else
        		assign dsizes = ''
            endif    
            assign msizes = '375,540,780,1080'
            
            assign imgPos = blkSt.ialign | replace:"_"," "
            if blkSt.image.presentation.focal_point != blank
               assign imgPos = blkSt.image.presentation.focal_point
            endif
            assign img2Pos = blkSt.ialign | replace:"_"," "
            if blkSt.imagem != blank and blkSt.imagem.presentation.focal_point != blank
               assign img2Pos = blkSt.imagem.presentation.focal_point
            endif
      %}
      
      <div class="ss_slide sslide{{ block.id }}" data-type="{{block.type}}" {% if block.type == 'video' %} data-video="{{videotype}}"{% endif %} {{ block.shopify_attributes }}>
        {% if blkSt.display_btn != true and blkSt.link != blank and block.type != 'video' %}<a href="{{ blkSt.link }}" class="ss_link" {% if blkSt.newt1 %}target="_blank"{% endif %}>{% endif %}

        {% if block.type == 'image' and blkSt.image != blank %}
              <img {% if section.index > 2 %} loading="lazy"{% endif %}class="ss_img lazyload js{% if blkSt.mb_image != blank %} small--hide{% else %}{% if forloop.first and section.index < 2 %} lazypreload{% endif %}{% endif %}" src="{{ blkSt.image | img_url: '100x' }}" data-srcset="{% render 'lazyload', image: blkSt.image, sizes: '400,760,1200,1600,2000' %}" data-sizes="auto" width="{{ blkSt.image.width }}" height="{{ blkSt.image.height }}" alt="{{ blkSt.image.alt }}" style="--pos:{{imgPos}};" />
              {% if blkSt.mb_image != blank %}
                  <img {% if section.index > 2 %} loading="lazy"{% endif %}class="ss_img lazyload js medium-up--hide{% if forloop.first and section.index < 2 %} lazypreload{% endif %}" src="{{ blkSt.mb_image | img_url: '100x' }}" data-srcset="{% render 'lazyload', image: blkSt.mb_image, sizes: '400,760,1200,1600,2000' %}" data-sizes="auto" width="{{ blkSt.mb_image.width }}" height="{{ blkSt.mb_image.height }}" alt="{{ blkSt.mb_image.alt }}" style="--ipos:{{img2Pos}};" />
          	  {% endif %}
        {% elsif block.type == 'video' and blkSt.video_url != blank %}
          {% if blkSt.video_url.type == "youtube" %}
        		<iframe class="embed-player slide-media" width="980" height="520" src="https://www.youtube.com/embed/{{ blkSt.video_url.id }}?enablejsapi=1&controls=0&fs=0&iv_load_policy=3&rel=0&showinfo=0&loop=1&playlist={{blkSt.video_url.id}}&playsinline=1" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
		  {% elsif blkSt.video_url.type == "vimeo" %}
        		<iframe class="embed-player slide-media" src="https://player.vimeo.com/video/{{blkSt.video_url.id}}?api=1&byline=0&portrait=0&title=0&background=1&mute=1&loop=1&autoplay=0&id={{blkSt.video_url.id}}" width="980" height="520" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
          {% endif %}
        {% elsif block.type == 'video' and blkSt.mp4url != blank %}
        	<video class="slide-video slide-media" loop muted preload="metadata" playsinline>
              <source src="{{blkSt.mp4url}}" type="video/mp4" />
            </video>
        {% else %}
        	<div class="ss_img js">
                <div class="ph_bg">{% capture current %}{% cycle 1, 2 %}{% endcapture %}{{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}</div>
            </div>
        {% endif %}
        {% if section.settings.ht == '1' %}<div class="ssimgHt">{% endif %}
            <div class="ss_txt-wrap{% if blkSt.title != blank or blkSt.ss_hd3 != blank %} ss_ol{% endif %} {{ blkSt.text_align }} {{ blkSt.text_align_m }}" style="max-width:{{blkSt.con_width }}px;{% if blkSt.cap_ol %}--stbg:{{ blkSt.cap_ol_cl | color_modify: 'alpha', blkSt.cap_ol_op }};{% endif %}{% if blkSt.cap_ol_mb %}--stbgm:{{ blkSt.cap_ol_cl | color_modify: 'alpha', blkSt.cap_ol_op}};{% endif %}--ovr:{{ blkSt.ss_ol | color_modify: 'alpha', blkSt.ss_ol_op }};">
              <div class="ss_txt{% if blkSt.text-left %} tl{% else %} tc{% endif %}{% if blkSt.cap_ol %} bg{% endif %}{% if blkSt.cap_ol_mb %} bgm{% endif %}{% if secSt.txtanimation %} anime {{ secSt.animation-style }}{% endif %}">
                 
    			  {% unless blkSt.ss_hd1 == blank %}<p class="ss_hd1 tltxt" style="--cl:{{blkSt.hd1_cl}};--fs:{{blkSt.hd1_fs}}px;--fw:{{blkSt.fw1}};--fsm:{{blkSt.hd1_fs_m}}px;--mb:{{blkSt.hd1_mb}}px;--mbm:{{blkSt.hd1_mb_m}}px;--ls:0.0{{ blkSt.hd1_ls}}em">
    			  {{ blkSt.ss_hd1 }}</p>{% endunless %}
                  
    			  {% unless blkSt.title == blank %}<h2 class="ss_hd2 tltxt" style="--ffm:var(--{{ blkSt.ffm }});--cl:{{blkSt.hd2_cl}};--fs:{{blkSt.hd2_fs}}px;--fw:{{blkSt.fw2}};--fsm:{{blkSt.hd2_fs_m}}px;--mb:{{blkSt.hd2_mb}}px;--mbm:{{blkSt.hd2_mb_m}}px;--ls:0.0{{ blkSt.hd2_ls}}em;--flh:{{blkSt.hd2_lh}}">
    			  {{ blkSt.title  }}</h2>{% endunless %}
                  
    			  {% unless blkSt.ss_hd3 == blank %}<div class="ss_hd3 tltxt{% if blkSt.ss_hd3_hide %} small--hide{% endif %}" style="--cl:{{blkSt.hd3_cl}};--fs:{{blkSt.hd3_fs}}px;--fw:{{blkSt.fw3}}px;--fsm:{{blkSt.hd3_fs_m}}px;--mb:{{blkSt.hd3_mb}}px;--mbm:{{blkSt.hd3_mb_m}}px;--ls:0.0{{ blkSt.hd3_ls}}em;--flh:{{blkSt.hd3_lh}}">												 
    			  {{ blkSt.ss_hd3 }}</div>{% endunless %}
    			  
                  {% if blkSt.display_btn %}
                  	  <div class="btnWrap">
                        <a class="btn {{blkSt.btn-size}} {{blkSt.btn-style}}" {% if blkSt.newt1 %}target="_blank"{% endif %} href="{{ blkSt.link }}">{{ blkSt.slide_button_text }}</a>
                        <a class="btn {{blkSt.btn-size}} {{blkSt.btn-style}}" {% if blkSt.newt2 %}target="_blank"{% endif %} href="{{ blkSt.link2 }}">{{ blkSt.slide_button_text_2 }}</a>
                  	  </div>
                  {% elsif block.type == 'video' %}
                	  {% if blkSt.slide_button_text != blank %}<div class="btnWrap"><a class="btn {{blkSt.btn-size}} {{blkSt.btn-style}}" href="{{ blkSt.link }}">{{blkSt.slide_button_text}}</a></div>{% endif %}
                  {% else %}
                    {% if blkSt.slide_button_text != blank %}<div class="btnWrap"><span class="btn {{blkSt.btn-size}} {{blkSt.btn-style}}" >{{blkSt.slide_button_text}}</span></div>{% endif %}
                  {% endif %}            
              </div>
            </div>
        {% if section.settings.ht == '1' %}</div>{% endif %}
        {% if blkSt.display_btn != true and blkSt.link != blank and block.type != 'video' %}</a>{% endif %}
      </div>
      {% endfor %}
    </div>
  </div>
  {% endif %}
  {% if section.blocks.size == 0 %}<div class="placeholder-noblocks">{{ 'homepage.onboarding.no_content' | t }}</div>{% endif %}
</div>
{%- endcapture -%}
