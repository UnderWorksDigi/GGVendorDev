        "default": "md"
      },
      {
        "type": "checkbox",
        "id": "enable_zoom",
        "label": "Enable Image Zoom?",
        "default": true
      }
    ],
  "blocks": [
	{
		"type": "hr",
		"name": "Divider/Space",
		"settings": [
			{
				"type":"color",
				"id":"hrc",
				"label":"Color",
				"default":"#222"
			},
			{
				"type": "range",
				"id": "mb",
				"min": 0,
				"max": 50,
				"step": 5,
				"label": "Bottom Margin",
				"unit": "px",
				"default": 20,
				"info":"You can also use this block to add more space between 2 blocks"
			}
		]
  },
  {
		"type": "title",
		"name": "Title",
		"limit": 1,
		"settings": [
		{
			"type": "color",
			"id": "cl",
			"label": "Color",
			"default": "#000"
		},
		{
			"type": "range",
			"id": "fs",
			"min": 10,
			"max": 60,
			"step": 1,
			"label": "Font Size",
			"unit": "px",
			"default": 22
		},
		{
			"type": "range",
			"id": "fsm",
			"min": 10,
			"max": 60,
			"step": 1,
			"label": "Font Size(mobile)",
			"unit": "px",
			"default": 18
		},
		 {
			"type": "range",
			"id": "lh",
			"label": "Line Height",
			"default": 1.3,
			"min": 1.0,
			"max": 2.0,
			"step": 0.1,
			"info": "Space between text lines"
        },
		{
			"type": "select",
			"id": "ff",
			"label": "Font Family",
			"default": "ft2",
			"options": [
			  {
				"value": "ft1",
				"label": "Font 1"
			  },
			  {
				"value": "ft2",
				"label": "Font 2"
			  },
			  {
